!!
!! XTERM
!!

XTerm*boldColors: false
XTerm*locale: true
!! Copy selection to X CLIPBOARD and X PRIMARY.
XTerm*selectToClipboard: true
XTerm*scrollBar: false


!!
!! TESTING
!!

Xft*antialias: true
Xft*dpi: 96

URxvt.scrollBar: false
URxvt.tintColor: Gray
URxvt*shading: 20
URxvt*fading: 0
URxvt.boldColors: false
URxvt.letterSpace: 0
URxvt.font: xft:Go Mono:size=10
URxvt.boldFont: xft:Go Mono:size=10
URxvt.iso14755: false
URxvt.iso14755_52: false
URxvt*internalBorder:       8
URxvt*matcher.button:       3
URxvt*matcher.rend.0:       Uline Bold fg5

urxvt*perl-lib: /home/rkk/lib/urxvt
!URxvt.perl-ext: matcher,plumb,keyboard-select
URxvt.perl-ext: plumb
URxvt.perl-ext-common: selection-to-clipboard
URxvt.url-launcher: /usr/bin/xdg-open

