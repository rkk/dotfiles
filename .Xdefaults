!!
!! XTERM
!!

XTerm*boldColors: false
XTerm*locale: true
!! UTF-8 values: 1 or 2
!! Copy selection to X CLIPBOARD and X PRIMARY.
XTerm*selectToClipboard: true
XTerm*scrollBar: false


!!
!! TESTING
!!

Xft*antialias: true
Xft*dpi: 96
urxvt.scrollBar: false
urxvt*tintColor: Gray
urxvt*shading: 20
urxvt*fading: 0
